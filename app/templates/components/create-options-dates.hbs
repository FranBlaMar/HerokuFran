{{#bs-form-element
  classNames='days'
  label=(t 'create.options.dates.label')
  property='options'
  as |value id|
}}
  {{bootstrap-datepicker-inline
    id=id
    value=optionsBootstrapDatepicker
    multidate=true
    calendarWeeks=true
    todayHighlight=true
    language=i18n.locale
  }}
{{/bs-form-element}}
