{
  "framework": "qunit",
  "test_page": "tests/index.html?hidepassed",
  "disable_watching": true,
  "browser_start_timeout": 30,
  "launch_in_ci": [
    "PhantomJS"
  ],
  "launch_in_dev": [
    "PhantomJS",
    "Chrome"
  ],
  "proxies": {
    "/": {
      "target": "http://localhost:4200",
      "onlyContentTypes": [
        "json"
      ]
    }
  },
  "launchers": {
    "SL_chrome": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "chrome",
        "-p",
        "Windows 10",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_firefox": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "firefox",
        "-p",
        "Windows 10",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_edge": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "microsoftedge",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_ie": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "internet explorer",
        "-v",
        "11",
        "-p",
        "Windows 10",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    },
    "SL_safari": {
      "exe": "ember",
      "args": [
        "sauce:launch",
        "-b",
        "safari",
        "-v",
        "9",
        "--vi",
        "public",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    }
  }
}
